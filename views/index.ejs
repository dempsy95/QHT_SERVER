<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>MY APP</title>
</head>
<body>
  May the force be with you
 <ul class="hotels">
  <% for(var i=0; i<hotels.length; i++) {%>
    <li class="hotel">
      <span><%= hotels[i].hotel_id %></span>
      <span><%= hotels[i].hotel_name%></span>
      <span><%= hotels[i].hotel_city%></span>
      <span><%= hotels[i].hotel_address%></span>
      <span><%= hotels[i].hotel_availability%></span>
    </li>
  <% } %>
  </ul>

  <form action="/hotels" method="POST">
    <input type="text" placeholder="hotel_id" name="hotel_id">
    <input type="text" placeholder="hotel_name" name="hotel_name">
    <input type="text" placeholder="hotel_city" name="hotel_city">
    <input type="text" placeholder="hotel_address" name="hotel_address">
    <input type="number" placeholder="hotel_availability" name="hotel_availability">
    <button type="submit">Submit</button>
  </form>

  <form action="/cities" method="POST">
    <input type="text" placeholder="city" name="city">
    <input type="text" placeholder="Hotel_01" name="Hotel_01">
    <input type="text" placeholder="Hotel_02" name="Hotel_02">
    <input type="text" placeholder="Hotel_03" name="Hotel_03">
    <button type="submit">Submit</button>
  </form>



  <div>
  <h2>Check in and check out</h2>
  <button id="update"> check in and check out </button>
</div>



</body>
</html>

<!-- ... -->
<script src="main.js"></script>
</body>